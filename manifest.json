{
  "manifest_version": 2,

  "name": "tabser",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
     "default_title": "Click here lmao!"
  },

  "background": {
        "scripts": ["addtab.js"],
        "persistent": false
    },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "alarms",
    "tabs"
  ]
}
